<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ink.dao.labelMapper">
    <resultMap id="BaseResultMap" type="labelResponse">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="label_name" jdbcType="VARCHAR" property="labelName" />
    </resultMap>
<!-- resultMap="BaseResultMap" -->
    <select id="getLabel" resultType="java.util.HashMap" >
        select id, label_name
        from label
    </select>

    <insert id="insertLabel" useGeneratedKeys="true" keyProperty="label.id" parameterType="label" >
        insert into label 
                (label_name) 
        VALUES (
                #{label.labelName, jdbcType=VARCHAR});
    </insert>
</mapper>